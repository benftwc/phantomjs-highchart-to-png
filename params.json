{
    chart: {
      polar: true,
      type: 'line'
    },
    title: {
      text: null,
      x: -80
    },
    colors: ['#222222'], 
    pane: {
      size: '80%',
      background: [{
        backgroundColor: 'none',
      }]
    },
    xAxis: {
      categories: [#labels#],
      tickmarkPlacement: 'on',
      lineWidth: 0,
      labels: {
        style: {
          font: '13px Arial',
          fontWeight: 'bold'
        },
      },
    },
    yAxis: {
      gridLineInterpolation: 'polygon',
      lineWidth: 0,
      min: #min#,
      max: #max#
    },
    tooltip: {
      shared: true,
      pointFormat: '<span style="color:{series.color}"><b>{point.y:,.0f}</b></span>'
    },
    legend: {
      align: 'right',
      verticalAlign: 'top',
      y: 70,
      layout: 'vertical'
    },
    series: [{
      name: 'legende',
      data: [#serie#],
      pointPlacement: 'on',
      showInLegend: false
    }]

  }
